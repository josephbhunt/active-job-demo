<h1>Your Photos!</h1>

<%= link_to "New", new_photo_path %>

</br></br>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Photo</th>
    </tr>
  </thead>
  <tbody>
    <%- @photos.each do |photo| %>
      <tr>
        <td><%= photo.title %></td>
        <td>
          <ul>
            <li>
              <% if photo.images.any? %>
                <%= image_tag photo.images.last %>
              <% end %>
              </li>
          </ul>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
